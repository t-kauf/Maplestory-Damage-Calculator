{
  "name": "maplestory-damage-calculator",
  "version": "1.0.0",
  "type": "module",
  "description": "Maplestory damage calculator with regression tests",
  "scripts": {
    "build": "node build.js",
    "start": "npm run build && python -m http.server 8000 || python3 -m http.server 8000 || npx http-server -p 8000",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:nav": "playwright test navigation.spec.js",
    "test:nav:headed": "playwright test navigation.spec.js --headed",
    "test:nav:debug": "playwright test navigation.spec.js --debug",
    "test:base-stats": "playwright test \"base-stats-.*\"",
    "test:base-stats:headed": "playwright test \"base-stats-.*\" --headed",
    "test:base-stats:debug": "playwright test \"base-stats-.*\" --debug",
    "test:base-stats:main": "playwright test base-stats-main.spec.js",
    "test:base-stats:main:headed": "playwright test base-stats-main.spec.js --headed",
    "test:base-stats:main:debug": "playwright test base-stats-main.spec.js --debug",
    "test:base-stats:mastery": "playwright test base-stats-mastery.spec.js",
    "test:base-stats:mastery:headed": "playwright test base-stats-mastery.spec.js --headed",
    "test:base-stats:mastery:debug": "playwright test base-stats-mastery.spec.js --debug",
    "test:base-stats:target": "playwright test base-stats-target-content.spec.js",
    "test:base-stats:target:headed": "playwright test base-stats-target-content.spec.js --headed",
    "test:base-stats:target:debug": "playwright test base-stats-target-content.spec.js --debug",
    "test:base-stats:coverage": "playwright test base-stats-coverage.spec.js",
    "test:base-stats:coverage:headed": "playwright test base-stats-coverage.spec.js --headed",
    "test:base-stats:coverage:debug": "playwright test base-stats-coverage.spec.js --debug",
    "test:weapons": "playwright test \"weapon-levels-.*\"",
    "test:weapons:headed": "playwright test \"weapon-levels-.*\" --headed",
    "test:weapons:debug": "playwright test \"weapon-levels-.*\" --debug",
    "test:weapons:main": "playwright test weapon-levels-main.spec.js",
    "test:weapons:main:headed": "playwright test weapon-levels-main.spec.js --headed",
    "test:weapons:main:debug": "playwright test weapon-levels-main.spec.js --debug",
    "test:weapons:priority": "playwright test weapon-levels-priority.spec.js",
    "test:weapons:priority:headed": "playwright test weapon-levels-priority.spec.js --headed",
    "test:weapons:priority:debug": "playwright test weapon-levels-priority.spec.js --debug",
    "test:weapons:coverage": "playwright test weapon-levels-coverage.spec.js",
    "test:weapons:coverage:headed": "playwright test weapon-levels-coverage.spec.js --headed",
    "test:weapons:coverage:debug": "playwright test weapon-levels-coverage.spec.js --debug",
    "test:pred": "playwright test \"pred.*\"",
    "test:pred:headed": "playwright test \"pred.*\" --headed",
    "test:pred:debug": "playwright test \"pred.*\" --debug",
    "test:pred:main": "playwright test predictions-main.spec.js",
    "test:pred:main:headed": "playwright test predictions-main.spec.js --headed",
    "test:pred:main:debug": "playwright test predictions-main.spec.js --debug",
    "test:pred:coverage": "playwright test predictions-coverage.spec.js",
    "test:pred:coverage:headed": "playwright test predictions-coverage.spec.js --headed",
    "test:pred:coverage:debug": "playwright test predictions-coverage.spec.js --debug",
    "test:pred:equivalency": "playwright test predictions-equivalency.spec.js",
    "test:pred:equivalency:headed": "playwright test predictions-equivalency.spec.js --headed",
    "test:pred:equivalency:debug": "playwright test predictions-equivalency.spec.js --debug",
    "test:lastFailed": "playwright test --last-failed",
    "lint": "eslint *.js",
    "lint:fix": "eslint *.js --fix"
  },
  "keywords": [
    "maplestory",
    "damage-calculator"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.57.0",
    "glob": "^13.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "chart.js": "^4.5.1"
  }
}
